{
  "name": "doctrine-file",
  "version": "1.0.1",
  "description": "File-Reader for the Doctrine JSDoc Parser",
  "main": "lib/index.js",
  "scripts": {
    "check": "npm run lint && npm run flow && npm test",
    "clean": "rimraf lib",
    "build": "babel src --out-dir lib --ignore __tests__",
    "flow": "flow",
    "lint": "eslint src",
    "precommit": "npm run check",
    "preversion": "npm run check",
    "prepublish": "npm run clean && npm run build",
    "test": "ava",
    "watch": "babel src --out-dir lib --ignore __tests__ --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/researchgate/doctrine-file.git"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "doctrine",
    "jsdoc",
    "parser"
  ],
  "author": "Daniel Tschinder <daniel@tschinder.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/researchgate/doctrine-file/issues"
  },
  "homepage": "https://github.com/researchgate/doctrine-file#readme",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.0.0",
    "babel-plugin-syntax-flow": "^6.13.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.14.0",
    "eslint": "^3.0.0",
    "eslint-config-airbnb-base": "^8.0.0",
    "eslint-plugin-import": "^1.8.0",
    "flow-bin": "^0.33.0",
    "husky": "^0.11.1",
    "rimraf": "^2.5.0"
  },
  "dependencies": {
    "doctrine": "^1.0.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
